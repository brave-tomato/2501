{
    "$schema": "http://json.schemastore.org/prettierrc",
    "jsxSingleQuote": false,
    "organizeImportsSkipDestructiveCodeActions": true,
    "overrides": [
        {
            "files": ".prettierrc",
            "options": {
                "parser": "json"
            }
        }
    ],
    "plugins": ["prettier-plugin-organize-imports"],
    "printWidth": 120,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all"
}
